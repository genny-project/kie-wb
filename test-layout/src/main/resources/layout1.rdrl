import java.lang.Number;
import genny.QEvent;
import genny.QCmd;

rule "layout1"
	dialect "mvel"
	ruleflow-group "LayoutRules"
	no-loop true
	when
		evt : QEvent( event_type == "EVT_LAYOUT" , code == "layout1" )
	then
		QCmd fact0 = new QCmd();
		fact0.setCmd_type( "CMD_LAYOUT" );
		fact0.setCode( "layout1" );
		insert( fact0 );
end
